{
  "name": "prettus/laravel-repository",
  "description": "Laravel - Repositories to  the database layer",
  "keywords": ["laravel", "repository", "eloquent", "model", "cache"],
  "license": "MIT",
  "authors": [
    {
      "name": "Anderson Andrade",
      "homepage": "http://andersonandra.de",
      "email": "contato@andersonandra.de",
      "role": "Developer"
    }
  ],
  "homepage": "http://andersao.github.io/l5-repository",
  "support": {
    "email": "contato@andersonandra.de",
    "issues":"https://github.com/masroore/laravel-repository/issues",
    "wiki":"https://github.com/masroore/laravel-repository",
    "source":"https://github.com/masroore/laravel-repository",
    "docs": "http://andersao.github.io/laravel-repository"
  },
  "repositories": [
      { "type": "git", "url": "https://github.com/masroore/laravel-validation" }
  ],
  "require": {
    "illuminate/http": "^6.0|^7.0|^8.0|^9.0",
    "illuminate/config": "^6.0|^7.0|^8.0|^9.0",
    "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
    "illuminate/database": "^6.0|^7.0|^8.0|^9.0",
    "illuminate/pagination": "^6.0|^7.0|^8.0|^9.0",
    "illuminate/console": "^6.0|^7.0|^8.0|^9.0",
    "illuminate/filesystem": "^6.0|^7.0|^8.0|^9.0",
    "prettus/laravel-validation": "dev-master",
    "illuminate/validation": "^6.0|^7.0|^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Prettus\\Repository\\": "src/Prettus/Repository/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "suggest": {
    "league/fractal": "Required to use the Fractal Presenter (0.12.*).",
    "robclancy/presenter": "Required to use the Presenter Model (1.3.*)",
    "prettus/laravel-validation": "Required to provide easy validation with the repository (1.1.*)"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Prettus\\Repository\\Providers\\RepositoryServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
